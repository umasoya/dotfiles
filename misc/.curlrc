## header
header = "Accept-Encoding: gzip, deflate, sdch"
header = "X-Config-File: ~/.curlrc"
# header = "User-Agent: Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/66.0.3359.117 Safari/537.36"

## cookie
cookie-jar = /tmp/.curl_cookies
cookie = /tmp/.curl_cookies

--compressed

## Redirect
-L
max-redirs = 10

## Allow unsafed SSL
-k

